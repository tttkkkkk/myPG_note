<h1>Please Add</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @note , url: confirm_new_note_path do |f| %>
      <div>
        <%= f.submit "Confirm", class: "btn btn-primary" %>
      </div>
      <div>
        <%= f.text_area :content , :size => "100x800"%>
      </div>
    <% end %>
  </div>
</div>
